   "source": [
    "from gekko import GEKKO\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# GEKKO model\n",
    "m = GEKKO(remote=False)\n",
    "\n",
    "\n",
    "# Time vector\n",
    "n_points = 150\n",
    "t = np.linspace(0, 10, n_points)\n",
    "m.time = t\n",
    "\n",
    "\n",
    "P_theta = m.MV(value=1.5, lb=1.0, ub=2.0)\n",
    "P_theta.STATUS = 1\n",
    "P_theta.DCOST = 0.1\n",
    "P_theta.DMAX = 0.05\n",
    "\n",
    "\n",
    "\n",
    "P_phi = m.MV(value=1.5, lb=1.0, ub=2.0)\n",
    "P_phi.STATUS = 1\n",
    "P_phi.DCOST = 0.1\n",
    "P_phi.DMAX = 0.05\n",
    "\n",
    "\n",
    "theta = m.CV(value = 0, lb=-90, ub=90)\n",
    "theta.STATUS = 1\n",
    "theta.TR_INIT = 1\n",
    "theta.TAU = 0.5\n",
    "\n",
    "phi = m.CV(value = 0, lb=-90, ub=90)\n",
    "phi.STATUS = 1 \n",
    "phi.TR_INIT = 1\n",
    "phi.TAU = 0.5\n",
    "\n",
    "theta.SP = 45 * np.sin(2 * np.pi * t / 10)\n",
    "phi.SP = 90 * (2 * (t / 10) - 1)\n",
    "\n",
    "\n",
    "m.Equation(m.abs(theta.dt()) <= 600)\n",
    "m.Equation(m.abs(phi.dt()) <= 600)\n",
    "\n",
    "# m.Equation(theta.dt() == -theta + 180*(P_theta - 1.5))\n",
    "# m.Equation(phi.dt() == -phi + 180*(P_phi - 1.5))\n",
    "\n",
    "m.Equation(theta == 180*(P_theta - 1.5))\n",
    "m.Equation(phi == 180*(P_phi - 1.5))\n",
    "\n",
    "a1,a2,a3,a4 = 10,10,1,1\n",
    "\n",
    "m.Obj(a1*(theta-m.Param(value=theta.SP))**2 + a2*(phi-m.Param(value=phi.SP))**2)\n",
    "m.Obj((a3*P_theta + a4*P_phi))\n",
    "\n",
    "m.options.IMODE = 6\n",
    "m.solve(disp=True)\n",
    "\n",
    "\n",
    "# Plot",
    "plt.figure(figsize=(10,8))",
    "plt.subplot(2,2,1)",
    "plt.plot(t, theta.value, 'r-', label='θ Output')",
    "plt.plot(t, theta.SP, 'k--', label=r"$θ_{target}$")",
    "plt.ylabel('Angle (deg)')",
    "plt.ylim([-50,50])",
    "plt.legend()",
    "plt.grid()",
    "plt.subplot(2,2,2)",
    "plt.plot(t, phi.value, 'b-',label='ϕ Output')",
    "plt.plot(t, phi.SP, 'k--', label=r"$ϕ_{target}$")",
    "plt.xlabel('Time (s)')",
    "plt.ylim([-100,100])",
    "plt.legend()",
    "plt.grid()",
    "plt.subplot(2,2,3)",
    "plt.plot(t, P_theta.value, color="orange",label=r"$P_{θ}$ Input")",
    "plt.ylabel('PWM (ms)')",
    "plt.ylim([1.2,1.8])",
    "plt.xlabel('Time (s)')",
    "plt.legend()",
    "plt.grid()",
    "plt.subplot(2,2,4)",
    "plt.plot(t, P_phi.value,'c-', label=r"$P_{ϕ}$ Input")",
    "plt.ylim([1,2])",
    "plt.xlabel('Time (s)')",
    "plt.legend()",
    "plt.grid()",
    "plt.tight_layout()",
    "plt.savefig("model.png")",
    "plt.show()"
   ]
  },
